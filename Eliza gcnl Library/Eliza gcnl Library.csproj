<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <!--<RootNamespace>Eliza_gcnl_Library</RootNamespace>-->
    <RootNamespace>Eliza_gcnl</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <!--<Nullable>enable</Nullable>-->
    <Nullable>disable</Nullable>
    <PackageId>Eliza_gcnl_Library</PackageId>
    <Title>Eliza gcnl Library</Title>
    <Company>Guille</Company>
    <Description>Librería para usar las clases de Google Cloud Natural Language para analizar textos</Description>
    <Copyright>Guillermo Som (Guille), 2023</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>Readme.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/elGuille-info/Eliza_MAUI/tree/master/Eliza%20gcnl%20Library</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Cloud Natural Language</PackageTags>
    <Version>1.0.1</Version>
  </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Google.Cloud.Language.V1" Version="3.2.0" />
    </ItemGroup>

    <ItemGroup>
      <None Update="Readme.md">
        <Pack>True</Pack>
        <PackagePath>\</PackagePath>
      </None>
    </ItemGroup>

</Project>
